            <JFXTextField fx:id="BOTTOM_TEXT" labelFloat="true" promptText="Initial value" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="15.0" />
               </VBox.margin>
            </JFXTextField>
            <BorderPane>
               <bottom>
                  <JFXButton fx:id="DONE" prefHeight="25.0" prefWidth="55.0" text="Done" BorderPane.alignment="CENTER_LEFT">
