<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.*?>
<JFXDialogLayout maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.gunivers.listgenerator.gui.functionality.SequenceController">
   <children>
      <VBox>
         <children>
            <JFXTextField fx:id="TOP_TEXT" labelFloat="true" promptText="Operation (put 'U' to use U(n-1))" VBox.vgrow="ALWAYS">
